//Pipeline generated with command line: ./imb-pip.pl --tasks-pip=1 --force
//By: ssayolsp At: Fr 17 Okt 2014 17:12:41 CEST
executor="lsf"
queue="short"
commands {
    FastQC {
        walltime="2:00"
        procs=1
        lsf_request_options="-app Reserve2G -J FastQC"
    }
    STAR_se {
        walltime="4:00"
        procs=4
        lsf_request_options="-R 'span[ptile=4]' -app Reserve10G -J star"
    }
    BAMindexer {
        walltime="1:00"
        procs=1
        lsf_request_options="-app Reserve1G -J BAMindexer"
    }
    htseq_count {
        walltime="4:00"
        procs=4
        lsf_request_options="-R 'span[ptile=4]' -app Reserve4G -J htseq_count"
    }
    subread_count {
        walltime="0:30"
        procs=4
        lsf_request_options="-R 'span[ptile=4]' -app Reserve300M -J subread_count"
    }
    bam2bw {
        walltime="1:00"
        procs=1
        lsf_request_options="-app Reserve2G -J bam2bw"
    }
    MarkDups {
        walltime="4:00"
        procs=1
        lsf_request_options="-app Reserve50G -J MarkDups"
    }
    dupRadar {
        walltime="2:00"
        procs=4
        lsf_request_options="-R 'span[ptile=4]' -app Reserve1G -J dupRadar"
    }
    geneBodyCov {
        walltime="5:00"
        procs=1
        lsf_request_options="-app Reserve1G -J geneBodyCoverage"
    }
    inferexperiment {
        walltime="2:00"
        procs=1
        lsf_request_options="-app Reserve1G -J inferexperiment"
    }
    qualimap {
        walltime="4:00"
        procs=1
        lsf_request_options="-app Reserve10G -J qualimap"
    }
    RNAtypes {
        walltime="1:00"
        procs=4
        lsf_request_options="-R 'span[ptile=4]' -app Reserve10G -J RNAtypes"
    }
    rnatypes {
        walltime="0:10"
        procs=4
        lsf_request_options="-R 'span[ptile=4]' -app Reserve300M -J rnatypes"
    }

    geneBodyCov2 {
        walltime="1:00"
        procs=4
        lsf_request_options="-R 'span[ptile=4]' -app Reserve6G -J geneBodyCov2"
    }
    DE_edgeR {
        walltime="1:00"
        procs=1
        lsf_request_options="-app Reserve6G -J DE.edgeR"
    }
    DE_DESeq2 {
        walltime="1:00"
        procs=1
        lsf_request_options="-W1:00 -n1 -app Reserve3G -J DE.DESeq2"
    }
    GO_Enrichment {
        walltime="1:00"
        procs=1
        lsf_request_options="-W1:00 -n1 -app  Reserve6G -J GO_Enrichment"
    }
    collectBpipeLogs {
        walltime="0:05"
        procs=1
        lsf_request_options="-app Reserve300M -J collectBpipeLogs"
    }
}
